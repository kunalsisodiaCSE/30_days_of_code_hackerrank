#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;

int main()
{
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    int t;
    cin >> t;
    while (t--)
    {
        string s;
        // getline(cin, s);
        // cin<<s;
        cin>>s;
        for (int i = 0; i < s.length(); i++)
        {
            if (i % 2 == 0)
            {
                cout << s[i];
            }
        }
        cout<<" ";
        for (int i = 0; i < s.length(); i++)
        {
            if (i % 2 != 0)
            {
                cout << s[i];
            }
        }
        cout<<"\n";
        // fflush(s);
    }
    return 0;
}
